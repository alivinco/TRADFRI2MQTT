[Unit]
Description=Ikea adapter
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
ExecStart=/usr/bin/java -Xms5m -jar ikea-ad.jar -conf connInfo.json  -broker tcp://localhost
User=fimp
WorkingDirectory=/opt/ikea-ad
Restart=on-failure
StandardOutput=null
StandardError=null

[Install]
WantedBy=multi-user.target